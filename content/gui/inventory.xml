	<HBox>
		<VBox>
			<Label name="CoinsLabel" text="Coins:" base_color="40, 40, 40"/>
			<Label name="LambWoolLabel" text="Lamb wool:" base_color="40, 40, 40"/>
			<Label name="TextileLabel" text="Textiles:" base_color="40, 40, 40"/>
			<Label name="BoardsLabel" text="Boards:" base_color="40, 40, 40"/>
			<Label name="FoodLabel" text="Food:" base_color="40, 40, 40"/>
